  <h2>ユーザー一覧</h2>
  <% @users.each do |user| %>
        <!--<div class="user_one col-md-3 col-sm-3 col-xs-3 ">-->
        <div class="user_one col-md-2 col-md-offset-2 col-sm-2 col-sm-offset-1">
          <div class="pro_img" >
            <p class="profile">
　　　          <%= profile_img(user)%>
            </p>
            <%= link_to user.name, user %><br>
               <%= render 'follow_form', user: user %>
          </div>
      </div>
  <% end %>
