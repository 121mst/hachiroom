<p id="notice">
<%= notice %>
</p>

<div class="container_index">
  <ul>
  <% @recipes.each do |recipe| %>
    <div class="elements">
      <%= link_to recipe_path(recipe) do %>
        <li>
          <div class="frame_01">
            <img src="<%= recipe.recipe_image %>" onError=this.src="<%= asset_path "no_image.png" %>">
          </div>
          <%= recipe.title %>
        </li>
      <% end %>
    </div>
  <% end %>
  </ul>
</div>
<div class="clear"><hr /></div>
